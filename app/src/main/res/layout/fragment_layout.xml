<dev.mfarm.com.mfarm.DashboardLayout

    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/app_bg"
    android:layout_weight="1"
     >


    <Button
        android:id="@+id/imgregister"
        android:drawableTop="@drawable/register"
        android:text="Register"
        style="@style/DashboardButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        />


    <Button
        android:id="@+id/imgmilk"
        android:drawableTop="@drawable/milkproduction"
        android:text="Milk Prod"
        style="@style/DashboardButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        />


    <Button
        android:id="@+id/imgmedication"
        android:drawableTop="@drawable/medication"
        android:text="Health"
        style="@style/DashboardButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        />
    <Button
        android:id="@+id/btn_reproduction"
        android:drawableTop="@drawable/medication"
        android:text="Send Data"
        style="@style/DashboardButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        />

    <Button
        android:id="@+id/imgmedication2"
        android:drawableTop="@drawable/medication"
        android:text="Health"
        style="@style/DashboardButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        />
    <Button
        android:id="@+id/btn_reproduction2"
        android:drawableTop="@drawable/medication"
        android:text="Send Data"
        style="@style/DashboardButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        />
 
</dev.mfarm.com.mfarm.DashboardLayout>